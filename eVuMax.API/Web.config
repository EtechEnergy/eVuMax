<configuration>
  <system.web.extensions>
    <scripting>
      <webServices>
        <!--Nishant Update this value to change the value to 
                    a larger value that can accommodate your JSON 
                    strings-->
        <jsonSerialization maxJsonLength="86753090"/>
      </webServices>
    </scripting>
  </system.web.extensions>
  <connectionStrings>
    <add name="VuMax" connectionString="Provider=SQLNCLI11;Data Source=localhost;User Id=sa;Password=123456;Trusted_Connection=yes;MARS Connection=True;Database=VuMaxDR"/>
  </connectionStrings>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*"/>
        <add name="Access-Control-Allow-Headers" value="Origin, X-Requested-With, Content-Type, Accept, Authorization"/>
        <add name="Access-Control-Allow-Methods" value="GET,POST,PUT,DELETE,OPTIONS"/>
        <add name="Access-Control-Allow-Credentials" value="true"/>
      </customHeaders>
    </httpProtocol>
    <security>
      <requestFiltering>
        <!--33000 31000 Nishant-->
        <requestLimits maxQueryString="2097151000" maxUrl="2097151000"/>
      </requestFiltering>
    </security>
    <directoryBrowse enabled="true"/>
    <validation validateIntegratedModeConfiguration="false"/>
    <modules>
      <remove name="TelemetryCorrelationHttpModule"/>
      <add name="TelemetryCorrelationHttpModule" type="Microsoft.AspNet.TelemetryCorrelation.TelemetryCorrelationHttpModule, Microsoft.AspNet.TelemetryCorrelation" preCondition="managedHandler"/>
    </modules>
  </system.webServer>
  <!--
